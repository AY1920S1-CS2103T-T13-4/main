<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
      prefHeight="400.0" prefWidth="600.0">

    <HBox spacing="5" VBox.vgrow="ALWAYS">
        <StackPane fx:id="folderStructurePlaceholder" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS"/>
        <StackPane fx:id="favoriteListPlaceholder" VBox.vgrow="ALWAYS" maxWidth="100"/>
    </HBox>

    <HBox VBox.vgrow="ALWAYS">
        <StackPane fx:id="reminderListPlaceholder" VBox.vgrow="ALWAYS"/>
    </HBox>
</VBox>
