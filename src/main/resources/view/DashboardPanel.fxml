<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="dashboardPanel">
    <HBox maxHeight="200">
        <HBox HBox.hgrow="ALWAYS">
            <StackPane fx:id="reminderListPlaceholder" HBox.hgrow="ALWAYS"/>
        </HBox>
        <HBox VBox.vgrow="ALWAYS" maxWidth="200">
            <StackPane fx:id="favoriteListPlaceholder"/>
        </HBox>
    </HBox>
    <HBox VBox.vgrow="ALWAYS">
        <VBox HBox.hgrow="ALWAYS">
            <AnchorPane>
                <Label text="Folders" textFill="ivory" fx:id="title" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
            </AnchorPane>
            <StackPane fx:id="folderStructurePlaceholder" HBox.hgrow="ALWAYS"/>
        </VBox>
        <HBox maxWidth="400">
            <StackPane fx:id="autotagDisplayPlaceholder"/>
        </HBox>
    </HBox>
</VBox>
