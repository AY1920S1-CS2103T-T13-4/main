@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor STORAGE_COLOR
skinparam classBackgroundColor STORAGE_COLOR

Interface Storage <<Interface>>
Interface UserPrefsStorage <<Interface>>
Interface MarkStorage <<Interface>>

Class StorageManager
Class JsonUserPrefsStorage
Class JsonMarkStorage

StorageManager .left.|> Storage
StorageManager o-right-> UserPrefsStorage
StorageManager o--> MarkStorage

JsonUserPrefsStorage .left.|> UserPrefsStorage
JsonMarkStorage .left.|> MarkStorage
JsonMarkStorage .down.> JsonSerializableMark
JsonSerializableMark .right.> JsonAdaptedBookmark
JsonSerializableMark .down> JsonAdaptedFolderStructure
JsonAdaptedBookmark .right.> JsonAdaptedTag
JsonAdaptedBookmark .up.> JsonAdaptedCachedCopy
@enduml
